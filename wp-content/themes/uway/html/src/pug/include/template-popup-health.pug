mixin template-popup-health
    doctype html
    html(lang="en")
        head
            meta(charset="UTF-8")
            meta(name="viewport", content="width=device-width, initial-scale=1, shrink-to-fit=no")
            meta(http-equiv="X-UA-Compatible", content="ie=edge")

            // Favicon
            link(rel="shortcut icon", href="upload/favicon.ico", type="image/x-icon")
            link(rel="apple-touch-icon", href="upload/favicon.png")

            // CSS
            link(rel="stylesheet", href="src/scss/plugins.css")
            link(rel="stylesheet", href="src/scss/style.css")

        body
            
            script sessionStorage.getItem("isFirstRun") || document.write('<div id="preloader"><div class="loading"></div></div>')

            

            // WRAP PAGE
            #wrap-page
                block

            script window.jQuery || document.write('<script src="src/js/jquery.js"><\/script>')
            script(src="src/js/plugins.min.js")
            script(data-src="https://maps.google.com/maps/api/js?libraries=places&key=AIzaSyApEpQ5tDwzlRfH1vuo4351Bwdar8OzyUI", id="google-map")
            script(src="src/js/main.js")
            script(src="src/js/initmap.js")

